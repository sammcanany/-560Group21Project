{
    "name": "560Group21Project",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sammcanany/560Group21Project.git"
    },
    "bugs": {
        "url": "https://github.com/sammcanany/560Group21Project/issues"
    },
    "homepage": "https://github.com/sammcanany/560Group21Project#readme",
    "scripts": {
        "build": "npm run css",
        "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
        "css-lint": "stylelint scss/",
        "css-prefix": "postcss --replace assets/css/starter.css --use autoprefixer --map",
        "css-purge": "purgecss --keyframes --css assets/css/starter.css --content index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output assets/css/",
        "css": "npm-run-all css-compile css-prefix",
        "server": "serve --listen 3000",
        "start": "npm-run-all --parallel watch server",
        "watch": "nodemon -e html,scss -x \"npm run css\"",
        "test": "npm run css-lint && npm run css"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.4",
        "bootstrap": "^5.1.3",
        "bootstrap-icons": "^1.8.1",
        "fontawesome-free": "^1.0.4",
        "jquery": "^3.6.0",
        "mssql": "^8.0.2"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.4",
        "node-sass": "^7.0.1",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.12",
        "postcss-cli": "^9.1.0",
        "purgecss": "^4.1.3",
        "serve": "^13.0.2",
        "stylelint": "^14.6.0",
        "stylelint-config-twbs-bootstrap": "^3.0.1"
    }
}
